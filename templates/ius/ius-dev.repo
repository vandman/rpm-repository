[ius-centos{{ item.releasever }}-dev]
name=IUS Community Packages for Enterprise Linux {{ item.releasever }} - {{ item.basearch }} - Dev
#baseurl=https://dl.iuscommunity.org/pub/ius/development/CentOS/{{ item.releasever }}/{{ item.basearch }}
mirrorlist=https://mirrors.iuscommunity.org/mirrorlist?repo=ius-centos{{ item.releasever }}-dev&arch={{ item.basearch }}&protocol=http
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ius

