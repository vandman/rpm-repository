failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ius

[ius-centos{{ item.releasever }}-archive-source]
name=IUS Community Packages for Enterprise Linux {{ item.releasever }} - {{ item.basearch }} - Archive Source
#baseurl=https://dl.iuscommunity.org/pub/ius/archive/CentOS/{{ item.releasever }}/SRPMS
